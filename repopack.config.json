{
  "output": {
    "filePath": "repopack-output.txt",
    "style": "plain",
    "removeComments": false,
    "removeEmptyLines": true,
    "topFilesLength": 5,
    "showLineNumbers": false
  },
  "include": ["runelite-api/src/main/java/net/runelite/api/**/*.java","runelite-client/src/main/java/net/runelite/client/plugins/microbot/example/**/*.java","runelite-client/src/main/java/net/runelite/client/plugins/VoxSylvaePlugins/**/*"],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": ["additional-folder", "**/*.log","runelite-api/src/main/java/net/runelite/api/**/*.java","runelite-client/src/main/java/net/runelite/client/plugins/questhelper/**/*.java"]
  },
  "security": {
    "enableSecurityCheck": true
  }
}